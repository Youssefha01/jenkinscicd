    agent any

    stages {
        stage('Hello') {
            steps {
                git 'https://github.com/Youssefha01/jenkinscicd.git'
            }
        }
        stage('BUILD') {
            steps {
                echo 'DEV'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Production'
            }
        }
    }
}
